{
  "name": "@common-ground-dao/cg-plugin-lib",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/common-ground-dao"
  },
  "version": "1.0.1",
  "description": "Client for interacting with Common Ground as a plugin",
  "author": "Common Ground",
  "license": "MIT",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": {
